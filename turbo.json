{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"]
    },
    "typecheck": { "outputs": [] },
    "lint": { "outputs": [] },
    "test": { "outputs": [] },
    "storybook": {
      "cache": false,
      "persistent": true
    },
    "e2e": {
      "cache": false
    }
  }
}