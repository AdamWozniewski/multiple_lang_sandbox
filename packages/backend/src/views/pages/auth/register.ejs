<h1>Rejestracja</h1>
<form method="POST">
    <div class="form-group">
        <label for="">Email</label>
        <input required value="<%= form?.email %>" type="email" name="email" class="form-control <%- errors?.email ? 'is-invalid' : '' %>%>
            " id="" aria-describedby="emailHelp" placeholder="Emial">
        <% if(errors?.email) { %>
            <div class="invalid-feedback"><%= errors.email.message %></div>
        <% } %>
    </div>
    <div class="form-group">
        <label for="">Hasło</label>
        <input required value="<%= form?.password %>" type="password" name="password" class="form-control <%- errors?.password ? 'is-invalid' : '' %>"
               id="" placeholder="Hasło">
        <% if(errors?.password) { %>
            <div class="invalid-feedback"><%= errors.password.message %></div>
        <% } %>
    </div>
    <button type="submit" class="btn btn-primary">Zarejestruj</button>
</form>